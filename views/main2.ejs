<% include partials/header %>
<table class="table table-hover">
  <tr>
    <th>Name</th>
    <th>Birthday</th>
    <th>Action</th>
    <th>Preference</th>
  </tr>
<%listed.forEach(function(val){%>
  <tr>
    <td><%= val.name %></td>
    <td><%= new Date(val.birthdate).toLocaleString() %></td>
    <td>
      <form class="" action="/update" method="post">
        <button type="submit" name="action" class="btn btn-success"value="update <%= val.id %>">Update</button>
        <button type="submit" name="action" class="btn btn-danger"value="delete <%= val.id %>">Delete</button>
        <button type="submit" name="action" class="btn btn-info"value="details <%= val.id %>">Details</button>
      </form>
    </td>
    <td><%= val.preference %></td>
  </tr>
<%})%>
</table>
<% include partials/footer %>
